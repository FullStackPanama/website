{
  "extends": "astro/tsconfigs/strict",
  "strictNullChecks": true,
  "compilerOptions": {
    "baseUrl": ".",
    "verbatimModuleSyntax": true,
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      },
    ],
    "paths": {
      "@/*": ["src/*"],
      "@memberImages/*": ["src/images/members/*"],
      "@companyLogos/*": ["src/images/companyLogos/*"],
      "@components/*": ["src/components/*"],
      "@content/*": ["src/content/*"],
      "@data/*": ["src/data_files/*"],
      "@images/*": ["src/images/*"],
      "@styles/*": ["src/styles/*"],
      "@utils/*": ["src/utils/*"]
    },
  }
}
